switch -glob [HTTP::uri] {
  "/images/*" { pool image_pool }
  "/videos/*" { pool video_pool }
  "/api" - "/api*" {
    set uri [string map -nocase {"/api" "/"} [HTTP::uri]]
    HTTP::uri $uri
  }
  "/healthcheck" {
    HTTP::host "api.google.com"
    node 10.0.0.1 443
  }
  default { pool default_pool }
}
